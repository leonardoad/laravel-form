{
  "name": "leonardoad/laravel-form",
  "description": "Form functionality for Laravel",
  "keywords": ["laravel", "form", "merkeleon"],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "5.*",
    "doctrine/dbal": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LeonardoAD\\Form\\Providers\\FormServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "LeonardoAD\\Form\\": "src/"
    }
  }
}
